                $("#meterialRoof").show();
            } else {
                $("#meterialRoof").hide();
                 $(".meterialAnsRoof").hide();
            }
        })
        $("#glass").change(function () {
            if ($('#glass').is(":checked") == true) {
                $("#meterialGlass").show();
            } else {
                $("#meterialGlass").hide();
