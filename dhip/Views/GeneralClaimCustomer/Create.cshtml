    function SendForm() {
        if ($('#agree').is(':checked')) {
            if ($('#floor').is(':checked')) {
                var floor = $('#floor').val()
                var meterialFloorSelect = $('#meterialFloorSelect').val()
                var piecesFloor = $('#piecesFloor').val()
                var unitFloor = $('#unitFloor').val()
            }
            if ($('#wall').is(':checked')) {
                var wall = $('#wall').val()
                var meterialWallSelect = $('#meterialWallSelect').val()
                var piecesWall = $('#piecesWall').val()
                var unitWall = $('#unitWall').val()
            }

            if ($('#ceiling').is(':checked')) {
                var ceiling = $('#ceiling').val()
                var meterialCeilingSelect = $('#meterialCeilingSelect').val()
                var piecesCeiling = $('#piecesCeiling').val()
                var unitCeiling = $('#unitCeiling').val()
            }

            if ($('#roof').is(':checked')) {
                var roof = $('#roof').val()
                var meterialRoofSelect = $('#meterialRoofSelect').val()
                var piecesRoof = $('#piecesRoof').val()
                var unitRoof = $('#unitRoof').val()
            }

            if ($('#glass').is(':checked')) {
                var glass = $('#glass').val()
                var meterialGlassSelect = $('#meterialGlassSelect').val()
                var piecesGlass = $('#piecesGlass').val()
                var unitGlass = $('#unitGlass').val()
            }

            if ($('#etc').is(':checked')) {
                var etc = $('#etc').val()
                var meterialEtcSelect = $('#meterialEtcSelect').val()
                var piecesEtc = $('#piecesEtc').val()
                var unitEtc = $('#unitEtc').val()
            }
            //if ($('#customFile').val() != "") {
            //    var file = $('#customFile').get(0).files;
            //    var dataImg = new FormData();
            //    dataImg.append("dataImg",file[0]);
            //}
            $.ajax(
                {
                    type: "POST",
                    url: '/GeneralClaimCustomer/Create',
                    data:
                    //JSON.stringify(data)
                    {
                        date: $('#date').val(),
                        incidentDate: $('#incidentDate').val(),
                        timeIncidentDate: $('#timeIncidentDate').val(),
                        nameClaim: $('#nameClaim').val(),
                        phoneClaim: $('#phoneClaim').val(),
                        emailClaim: $('#emailClaim').val(),
                        nameContact: $('#nameContact').val(),
                        phoneContact: $('#phoneContact').val(),
                        emailContact: $('#emailContact').val(),
                        insuranceNo: $('#insuranceNo').val(),
                        place: $('#place').val(),
                        soi: $('#soi').val(),
                        road: $('#road').val(),
                        province: $('#province').val(),
                        typeHouse: $('#typeHouse').val(),

                        floor: floor,
                        wall: wall,
                        ceiling: ceiling,
                        roof: roof,
                        glass: glass,
                        etc: etc,

                        meterialFloorSelect: meterialFloorSelect,
                        piecesFloor: piecesFloor,
                        unitFloor: unitFloor,
                        meterialWallSelect: meterialWallSelect,
                        piecesWall: piecesWall,
                        unitWall: unitWall,
                        meterialCeilingSelect: meterialCeilingSelect,
                        piecesCeiling: piecesCeiling,
                        unitCeiling: unitCeiling,
                        meterialRoofSelect: meterialRoofSelect,
                        piecesRoof: piecesRoof,
                        unitRoof: unitRoof,
                        meterialGlassSelect: meterialGlassSelect,
                        piecesGlass: piecesGlass,
                        unitGlass: unitGlass,
                        meterialEtcSelect: meterialEtcSelect,
                        piecesEtc: piecesEtc,
                        unitEtc: unitEtc,

                        incidentPrice: parseInt($('#incidentPrice').val()),

                        causeIncident: $('#causeIncident').val(),
                        causeIncidentDetail: $('#causeIncidentDetail').val(),

                        //dataImg: file,
                    }
                    ,
                    success: function (result) {
                        alert("Success")
                        window.location.href = '@Url.Action("Index", "GeneralClaimCustomer")';
