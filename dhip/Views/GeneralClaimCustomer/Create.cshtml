    function GetTypeHouse() {
        $("#typeHouse").change(function () {
            var test = this.value;
            if (test == 0) {
                $("#meterialChk").hide();
                $("#meterialFloor").hide();
                $("#meterialWall").hide();
                $("#meterialCeiling").hide();
                $("#meterialRoof").hide();
                $("#meterialGlass").hide();
                $("#meterialEtc").hide();
                $(".meterialAns").hide();
            }
            if (test != 0) {
                $("#meterialChk").show();
            }
        });

    }
    function GetTypeJob() {
        $("#floor").change(function () {

            if ($('#floor').is(":checked") == true) {
                $("#meterialFloor").show();
            } else {
                $("#meterialFloor").hide();
                 $(".meterialAnsFloor").hide();
            }
        })
        $("#wall").change(function () {
            if ($('#wall').is(":checked") == true) {
                $("#meterialWall").show();
            } else {
                $("#meterialWall").hide();
                 $(".meterialAnsWall").hide();
            }
        })
        $("#ceiling").change(function () {
            if ($('#ceiling').is(":checked") == true) {
                $("#meterialCeiling").show();
            } else {
                $("#meterialCeiling").hide();
                 $(".meterialAnsCeiling").hide();
            }
